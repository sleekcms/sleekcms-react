{
  "name": "sleekcms-react",
  "version": "0.1.3",
  "description": "React bindings for @sleekcms/client",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "npm run clean && tsup src/index.tsx --dts --format esm,cjs --out-dir dist",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "sleekcms-client": "^0.1.5"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "rimraf": "^5.0.5",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
