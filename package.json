{
  "name": "@sleekcms/react",
  "version": "1.4.1",
  "description": "React bindings for @sleekcms/client",
  "main": "index.cjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "build": "tsup src/index.tsx --dts --format esm,cjs && npm run copy-files",
    "copy-files": "cp README.md package.json .npmrc dist/",
    "clean": "rimraf dist || true",
    "test": "vitest run"
  },
  "files": [
    "*.js",
    "*.mjs",
    "*.d.ts",
    "*.d.mts",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sleekcms/sleekcms-react.git",
    "directory": "packages/sleekcms-react"
  },
  "homepage": "https://github.com/sleekcms/sleekcms-react/#readme",
  "bugs": {
    "url": "https://github.com/sleekcms/sleekcms-react/issues"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@sleekcms/client": "^2.2.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "jsdom": "^27.4.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
